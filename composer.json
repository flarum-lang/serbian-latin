{
	"name": "flarum-lang/serbian",
	"type": "flarum-extension",
	"description": "Serbian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/serbian",
	"keywords": [
		"locale",
		"flarum",
		"sr-locale"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/serbian/issues",
		"source": "https://github.com/flarum-lang/serbian"
	},
	"require": {
		"flarum/core": "^1.4"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Serbian",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "sr",
			"title": "Serbian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
