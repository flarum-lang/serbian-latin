{
	"name": "flarum-lang/serbian-latin",
	"type": "flarum-extension",
	"description": "Serbian (Latin) language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/serbian-latin",
	"keywords": [
		"locale",
		"flarum",
		"sr-locale",
		"sr_Latn-locale"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/serbian-latin/issues",
		"source": "https://github.com/flarum-lang/serbian-latin"
	},
	"require": {
		"flarum/core": "^1.4"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Serbian (Latin)",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "sr-Latn",
			"title": "Serbian (Latin)"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
